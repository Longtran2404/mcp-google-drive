{
  "name": "mcp-google-drive",
  "version": "1.4.1",
  "description": "Advanced MCP server for Google Drive integration with full CRUD operations, file management, and sharing capabilities. Supports both OAuth2 and Service Account authentication.",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "mcp-google-drive": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepublishOnly": "npm run build",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts",
    "clean": "rimraf dist",
    "rebuild": "npm run clean && npm run build",
    "prepare": "npm run build"
  },
  "keywords": [
    "mcp",
    "google-drive",
    "google",
    "drive",
    "api",
    "oauth2",
    "authentication",
    "file-management",
    "cloud-storage",
    "mcp-server",
    "model-context-protocol",
    "typescript",
    "nodejs"
  ],
  "author": "MCP Google Drive Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Longtran2404/mcp-google-drive.git"
  },
  "bugs": {
    "url": "https://github.com/Longtran2404/mcp-google-drive/issues"
  },
  "homepage": "https://github.com/Longtran2404/mcp-google-drive#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.4.0",
    "googleapis": "^128.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.1",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0"
  }
}
